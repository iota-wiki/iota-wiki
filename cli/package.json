{
  "name": "@iota-community/iota-wiki-cli",
  "description": "A command line utility to manage Wiki content and preview content locally.",
  "author": "IOTA community",
  "license": "MIT",
  "version": "1.7.0",
  "homepage": "https://github.com/iota-community/iota-wiki-cli",
  "bugs": "https://github.com/iota-community/iota-wiki-cli/issues",
  "repository": "iota-community/iota-wiki-cli",
  "main": "dist/index.js",
  "bin": {
    "iota-wiki": "dist/index.js"
  },
  "scripts": {
    "build": "tsc --build",
    "watch": "yarn build --watch",
    "format": "prettier --write .",
    "lint": "eslint --cache --fix .",
    "prepack": "yarn build",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.18",
    "@yarnpkg/shell": "^3.2.0",
    "axios": "^0.26.1",
    "clipanion": "^3.2.0-rc.10",
    "ink": "^3.2.0",
    "ink-multi-select": "^2.0.0",
    "ink-text-input": "^4.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.9.4",
    "@types/prettier": "^2",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": ">=6",
    "lint-staged": ">=10",
    "pinst": ">=2",
    "prettier": "2.6.0",
    "typescript": "^4.4.3"
  },
  "files": [
    "/dist",
    "/internal"
  ],
  "workspaces": [
    "internal",
    "test/*"
  ],
  "types": "dist/index.d.ts",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,json,yml,md,mdx,css}": "prettier --write"
  },
  "packageManager": "yarn@3.2.0",
  "engines": {
    "node": ">=14.0.0"
  }
}
